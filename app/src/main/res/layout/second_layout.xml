<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/lineaL1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/purple_500"
    android:orientation="vertical">

    <EditText
        android:id="@+id/textedit1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:hint="@string/input"
        android:autofillHints="" />

    <EditText
        android:id="@+id/textedit2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPersonName"
        android:hint="@string/input"
        android:autofillHints="" />
    <EditText
        android:id="@+id/textedit3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="phone"
        android:hint="@string/input"
        android:autofillHints="" />
    <EditText
        android:id="@+id/textedit4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:hint="@string/input"
        android:autofillHints="" />
    <EditText
        android:id="@+id/textedit5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textPassword"
        android:hint="@string/input"
        android:autofillHints="" />
    <EditText
        android:id="@+id/textedit6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textEmailAddress"
        android:hint="@string/input"
        android:autofillHints="" />
    <EditText
        android:id="@+id/textedit7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textUri"
        android:hint="@string/input"
        android:autofillHints="" />
    <EditText
        android:id="@+id/textedit8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="textCapWords"
        android:hint="@string/input"
        android:autofillHints="" />
    <EditText
        android:id="@+id/textedit9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="date"
        android:hint="@string/input"
        android:autofillHints="" />
    <CalendarView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
</LinearLayout>



